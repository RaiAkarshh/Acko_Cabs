<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000000"
    tools:context=".AboutUs">

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="303dp"
        android:layout_height="143dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.022"
        app:srcCompat="@drawable/acko_cabs" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="362dp"
        android:layout_height="439dp"
        android:fontFamily="@font/alatsi"
        android:text="Experience Acko Cabs, the ultimate tourist cab app for Delhi, Mumbai, Kashmir, Chennai, and Hyderabad. With top-notch service and well-maintained vehicles, navigate these captivating cities with ease. Book a cab, sit back, and embark on a seamless travel adventure. Download now for an unforgettable journey.Founded by Akarsh and Chirag from Sahyadri College, Mangalore"
        android:textColor="#776324"
        android:textSize="24sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.403" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/phonebtn"
        android:layout_width="212dp"
        android:layout_height="62dp"
        android:background="#776324"
        android:text="CALL US"
        android:drawableLeft="@drawable/phone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.819" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/mailbtn"
        android:layout_width="212dp"
        android:layout_height="62dp"
        android:background="#776324"
        android:text="E-MAIL US"
        android:drawableLeft="@drawable/email"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.923" />
</androidx.constraintlayout.widget.ConstraintLayout>